// 既定値を上書きするには、このファイル内にキー バインドを挿入します
[
    {
        "key": "cmd+f1",
        "command": "latex-workshop.tab",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+alt+t",
        "command": "-latex-workshop.tab",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+[BracketLeft]",
        "command": "workbench.action.terminal.new"
    },
    {
        "key": "ctrl+shift+`",
        "command": "-workbench.action.terminal.new"
    }
]